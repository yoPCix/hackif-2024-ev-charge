  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:css\" \"bun run dev:react\"",
    "npm:dev": "concurrently \"npm run dev:css\" \"npm run dev:react\"",
    "dev:react": "vite",
    "dev:css": "tailwindcss -i ./src/assets/styles/index.css -o ./src/assets/styles/output.css --watch",
