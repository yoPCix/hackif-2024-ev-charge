    "@ids/style-scaffolding": "^18.20.0",
    "@react-google-maps/api": "^2.19.3",
    "@t3-oss/env-core": "^0.11.0",
    "axios": "^1.7.4",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.1",
    "tw-merge": "^0.0.1-alpha.3",
    "zod": "^3.23.8"
